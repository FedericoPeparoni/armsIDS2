                </structure>
                <structure>
                    <property name="position">20</property>
                    <property name="name">billing_invoice_number</property>
                    <property name="nativeName">billing_invoice_number</property>
                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[SELECT a.id AS account_id,
       a.NAME                             AS account_name,
       
       a.aviation_billing_mailing_address AS aviation_billing_mailing_address,
       a.aviation_billing_contact_person_name AS person_name,
	   a.aviation_billing_phone_number        AS phone_number,
	   a.aviation_billing_email_address       AS email_address,
	   lng.val                     			  AS account_type,
	   
       COALESCE (x.opening_balance, 0)    AS opening_balance,
       x.transaction_type_id 			  AS transaction_type_id,
       x.description					  AS description,
       x.symbol 						  AS symbol,
       x.payment_mechanism				  AS payment_mechanism,
       x.payment_reference_number 		  AS payment_reference_number,
       x.balance						  AS balance,
       x.amount                			  AS amount,
       x.transaction_type 				  AS transaction_type,
       x.transaction_date_time 			  AS transaction_date_time,
       x.exchange_rate_to_usd			  AS exchange_rate_to_usd,
       x.invoice_type                     AS billing_ledgers_invoice_type,
       x.billing_centers_name           AS billing_centers_name,
       x.billing_invoice_number           AS billing_invoice_number
FROM   accounts a
       INNER JOIN (
       SELECT DISTINCT a.id                    		AS account_id,
                       original.latest_balance 		AS opening_balance,
                       original.id             		AS opening_balance_transaction_id,
                       CASE
                           WHEN original.id IS NOT NULL
                           OR t.id IS NOT NULL THEN
                                c.currency_code
                           ELSE NULL
                           END                  	AS symbol,
                        t.id                    	AS transaction_id,
                        t.transaction_date_time 	AS transaction_date_time,
                        t.amount                	AS amount,
                        t.balance               	AS balance,
                        t.transaction_type_id 		AS transaction_type_id,
                        t.description				AS description,
                        t.payment_mechanism			AS payment_mechanism,
                        t.payment_reference_number 	AS payment_reference_number,
                        t.exchange_rate		 		AS exchange_rate_to_usd,
                        tt.NAME                 	AS transaction_type,
                        t.invoice_type              AS invoice_type,
                        t.billing_centers_name      AS billing_centers_name,
                        t.billing_invoice_number      AS billing_invoice_number
                        FROM   abms.accounts a
                               CROSS JOIN abms.currencies c
                               LEFT OUTER JOIN (SELECT t.id,
                                                       t.account_id,
                                                       t.currency_id,
                                                       t.balance latest_balance
                                                FROM   abms.transactions t
                               JOIN (SELECT account_id,
                                            currency_id,
                                            Max(id) id
                                     FROM   abms.transactions

                                WHERE  transaction_date_time < ?

                                     GROUP  BY account_id,
                                               currency_id
                                     ORDER  BY account_id) t1
                                 ON T.id = t1.id) AS original
                                            ON original.account_id = a.id
                                               AND original.currency_id = c.id
                               LEFT OUTER JOIN
                               (

SELECT *
  FROM (
SELECT t.account_id, bl.invoice_number, t.transaction_date_time, t.amount, t.balance, bl.invoice_type, t.payment_mechanism, t.payment_reference_number, t.currency_id, t.transaction_type_id, t.id, CONCAT(t.description, ' (', EXTRACT (YEAR FROM bl.invoice_period_or_date), ') ') AS description, t.exchange_rate,  be.name  AS billing_centers_name, ('') AS billing_invoice_number
FROM   abms.transactions AS t
INNER JOIN abms.billing_ledgers bl ON bl.invoice_number = t.payment_reference_number
INNER JOIN abms.billing_centers be ON bl.billing_center_id = be.id

UNION
SELECT  bl.account_id, bl.invoice_number, t.transaction_date_time, t.amount, t.balance, bl.invoice_type, t.payment_mechanism, t.payment_reference_number, t.currency_id, t.transaction_type_id, t.id, t.description AS description, t.exchange_rate, be.name  AS billing_centers_name, bl.invoice_number AS billing_invoice_number
FROM abms.billing_ledgers bl
INNER JOIN abms.transaction_payments tp ON bl.id = tp.billing_ledger_id
INNER JOIN abms.transactions t ON tp.transaction_id = t.id
INNER JOIN abms.billing_centers be ON bl.billing_center_id = be.id
) AS t
                                WHERE  t.transaction_date_time
                                       >= ?
                                       AND t.transaction_date_time <= ?

                             ) AS T
                                            ON a.id = t.account_id
                                               AND T.currency_id = c.id

                               LEFT OUTER JOIN abms.transaction_types tt
                                            ON tt.id = T.transaction_type_id
                        WHERE  ( original.latest_balance IS NOT NULL  AND
                                               ((T.invoice_type='unified-tax' AND (T.payment_mechanism='invoice' OR  T.payment_mechanism='adjustment' OR T.payment_mechanism='wire' OR T.payment_mechanism='cash' OR T.payment_mechanism='debit' OR T.payment_mechanism='credit' OR T.payment_mechanism='cheque' ) )) OR t.id IS NOT NULL )) x 
                    ON a.id = x.account_id
LEFT OUTER JOIN abms.account_types 
ON abms.account_types.id = a.account_type

LEFT OUTER JOIN (SELECT DISTINCT abms.languages.token, abms.languages.code, abms.languages.val FROM abms.languages) as lng
ON abms.account_types.name = lng.token AND lng.code LIKE 'es'

WHERE  a.id = any(?::int[])

ORDER  BY a.id,
          transaction_id,
          symbol]]></xml-property>
        </oda-data-set>
