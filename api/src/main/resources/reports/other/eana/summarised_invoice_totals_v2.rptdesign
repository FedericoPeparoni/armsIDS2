            <xml-property name="queryText"><![CDATA[SELECT          a.id                                                                  AS account_id,
                a.NAME                                                                AS account_name,
                b.invoice_period_or_date                                              AS invoice_period_or_date,
                b.invoice_date_of_issue                                               AS invoice_date_of_issue,
                ( b.invoice_amount * b.invoice_exchange )                             AS invoice_amount_usd,
                ( b.invoice_amount * b.invoice_exchange_to_ansp )                     AS invoice_amount_ansp,
                (( b.invoice_amount - b.amount_owing ) * b.invoice_exchange )         AS amount_paid_usd,
                (( b.invoice_amount - b.amount_owing ) * b.invoice_exchange_to_ansp ) AS amount_paid_ansp,
                ( b.amount_owing * b.invoice_exchange )                               AS amount_owing_usd,
                ( b.amount_owing * b.invoice_exchange_to_ansp )                       AS amount_owing_ansp,
                CASE 
					WHEN Count(overdue_b.id) > 0 THEN 'overdue' 
					ELSE 'current' 
                END                                                					  AS account_status, 
                payment.created_at                                 					  AS last_payment_date, 
                ( payment.amount * payment.exchange_rate )         					  AS last_payment_amount_usd, 
                ( payment.amount * payment.exchange_rate_to_ansp ) 					  AS last_payment_amount_ansp 
FROM            abms.billing_ledgers b 
LEFT JOIN       abms.accounts a 
ON              a.id = b.account_id 
LEFT OUTER JOIN abms.billing_ledgers overdue_b 
ON              b.account_id = overdue_b.account_id 
AND             (overdue_b.payment_due_date::timestamp with time zone + interval '1 day') < ? 
AND             overdue_b.amount_owing > 0 
LEFT OUTER JOIN 
				(SELECT DISTINCT 
				ON (a.id) a.id, 
						   tp.created_at, 
						   tp.amount, 
						   tp.exchange_rate, 
						   tp.exchange_rate_to_ansp 
				FROM       abms.transaction_payments tp 
				JOIN       abms.billing_ledgers b 
				ON         b.id = tp.billing_ledger_id 
				JOIN       abms.accounts a 
				ON         b.account_id = a.id 
				WHERE      tp.created_at < ? 
				AND        a.id = any(?::int[]) 
				ORDER BY   a.id, tp.created_at DESC) payment 
ON              payment.id = a.id 
WHERE           (b.invoice_state_type = 'PUBLISHED' OR b.invoice_state_type = 'PAID') 
AND             b.invoice_period_or_date > ? 
AND             b.invoice_period_or_date < ? 
AND             a.id = any(?::int[])
GROUP BY        a.id, 
                a.NAME, 
                b.invoice_period_or_date, 
                b.invoice_date_of_issue, 
                b.invoice_exchange, 
                b.invoice_exchange_to_ansp, 
                b.invoice_amount, 
                b.invoice_amount, 
                b.amount_owing,  
                payment.created_at, 
                payment.amount, 
                payment.exchange_rate, 
                payment.exchange_rate_to_ansp 
ORDER BY        a.NAME
