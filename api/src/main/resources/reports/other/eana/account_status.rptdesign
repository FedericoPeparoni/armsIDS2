            <xml-property name="queryText"><![CDATA[SELECT 
	sub.*, 
	CASE WHEN sub.status = 'Over Limit' THEN sub.status 
		 WHEN overdue_count > 0 THEN 'Overdue'
		 ELSE 'Current' END AS real_status,
	CASE WHEN overdue_count > 0 THEN sub.longest_overdue 
		 ELSE null END AS real_longest_overdue,
	CASE WHEN overdue_count > 0 THEN sub.amount_owing 
		 ELSE 0 END AS real_overdue_amount

FROM (
	SELECT 
		a.id AS account_id,
		a.name AS account_name,
		a.credit_limit AS credit_limit,
		a.cash_account AS cash_account,
		COUNT(b.id) AS invoice_count,
		SUM(b.invoice_amount * b.invoice_exchange_to_ansp) AS invoice_amount,
		SUM(b.amount_owing * b.invoice_exchange_to_ansp) AS amount_owing,
		MIN(b.payment_due_date) AS longest_overdue,
		(SELECT ts.transaction_date_time FROM transactions ts WHERE ts.transaction_type_id = 1 AND ts.account_id = 243 ORDER BY transaction_date_time DESC LIMIT 1) AS last_payment,
		(SELECT ts.balance FROM transactions ts WHERE ts.account_id = a.id ORDER BY transaction_date_time DESC LIMIT 1) AS balance,
		SUM(CASE WHEN b.amount_owing > 0 AND b.payment_due_date < now() THEN 1 ELSE 0 END) AS overdue_count,
		CASE WHEN SUM(b.amount_owing * b.invoice_exchange_to_ansp) > a.credit_limit THEN 'Over Limit' ELSE 'Unknown' END AS status

	FROM
		accounts a
		LEFT JOIN billing_ledgers b ON a.id = b.account_id
		 

	WHERE 
		a.active = true
		
	GROUP BY 
		a.id,
		a.name,
		a.credit_limit
	) sub

WHERE 
	CASE WHEN ? = true THEN sub.cash_account = false 
		 ELSE true = true END

ORDER BY 
	account_name
]]></xml-property>
