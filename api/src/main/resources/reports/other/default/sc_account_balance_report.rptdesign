            <xml-property name="queryText"><![CDATA[SELECT   a.id                                AS account_id, 
         a.NAME                              AS account_name, 
         a.whitelist_last_activity_date_time AS last_activity, 
         a.whitelist_state                   AS status, 
         ( 
                SELECT current_value 
                FROM   system_configurations 
                WHERE  item_name = 'Inactivity period') AS inactivity_period, 
         ( 
                SELECT current_value 
                FROM   system_configurations 
                WHERE  item_name = 'Expiry period') AS expiry_period, 
         ( 
                  SELECT   t.balance 
                  FROM     transactions t 
                  WHERE    t.account_id = a.id 
                  AND      transaction_date_time < ?
                  AND      t.currency_id = 
                           ( 
                                  SELECT id 
                                  FROM   currencies 
                                  WHERE  currency_code = 'USD') 
                  ORDER BY t.transaction_date_time DESC limit 1) AS balance, 
         ( 
                SELECT sum(total_charges_usd) 
                FROM   flight_movements 
                WHERE  account = a.id 
                AND    status IN ( 'PENDING', 
                                  'INCOMPLETE' ) 
                AND    date_of_flight BETWEEN ? AND ?) 			AS owing 
FROM     accounts a 
WHERE    a.id IN (SELECT distinct account_id FROM account_users_map)
	AND (?::varchar is null or length(?::varchar) = 0 or a.whitelist_state = any(?::varchar[]))
ORDER BY a.NAME]]></xml-property>
