            <xml-property name="queryText"><![CDATA[select 
	id,
	charge_class,
	category,
	type,
	description,
	amount,
	(select currency_name 
		from currencies 
		where id=(select invoice_currency from accounts where accounts.id=?)
	) as CURRENCY_NAME,
	(select currency_code 
		from currencies 
		where id=(select invoice_currency from accounts where accounts.id=?)
	) as CURRENCY_CODE
from 
	service_charge_catalogues
]]></xml-property>
