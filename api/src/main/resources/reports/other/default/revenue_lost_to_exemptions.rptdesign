            <xml-property name="queryText"><![CDATA[SELECT 
	a.id,
	a.name,
	sum(fm.exempt_enroute_charges) as exempt_enroute_charges,
	sum(fm.exempt_approch_charges) as exempt_approch_charges,
	sum(fm.exempt_dep_charges) as exempt_dep_charges,
	sum(fm.exempt_late_charges) as exempt_late_charges,
	sum(fm.exempt_parking_charges) as exempt_parking_charges,
	sum(fm.exempt_domestic_passenger_charges) as exempt_domestic_passenger_charges,
	sum(fm.exempt_international_passenger_charges) as exempt_international_passenger_charges
FROM
	abms.flight_movements fm INNER JOIN abms.accounts a ON a.id = fm.account
WHERE
	fm.date_of_flight > ? and fm.date_of_flight < ?]]></xml-property>
