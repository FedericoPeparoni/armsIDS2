            <xml-property name="queryText"><![CDATA[SELECT 	   
	fm.flight_id 				as flight_id,
	fm.date_of_flight::date 	as day_of_flight, 
	fm.item18_reg_num 			as registration_number, 
	fm.movement_type			as movement_type, 
	fm.dep_ad					as dep_ad, 
	fm.dep_time 				as departure_time, 
	fm.dest_ad					as dest_ad,
	fm.account					as account_id,
	a.name 						as account_name
FROM 	abms.flight_movements as fm
	LEFT OUTER JOIN abms.accounts as a on
		fm.account = a.id
	LEFT OUTER JOIN abms.flight_movements as preceeding on
		preceeding.date_of_flight >= (fm.date_of_flight - '7 days'::interval) AND
		fm.dep_ad = preceeding.dest_ad AND
		fm.item18_reg_num = preceeding.item18_reg_num AND
		fm.id <> preceeding.id
WHERE
	preceeding.id is null AND
	fm.movement_type in ('DOMESTIC', 'INT_DEPARTURE', 'REG_DEPARTURE') AND
	fm.date_of_flight between ?::timestamp and ?::timestamp
	AND (?::varchar is null or length(?::varchar) = 0 or fm.account = any(?::int[]))
	AND (?::varchar is null or length(?::varchar) = 0 or fm.item18_reg_num = any(?::varchar[]))
ORDER BY (CASE WHEN (?) THEN fm.account::varchar ELSE '' END) DESC, fm.item18_reg_num, fm.date_of_flight, fm.dep_time;]]></xml-property>
