            <xml-property name="queryText"><![CDATA[
SELECT  abms.accounts.id AS account_id, abms.accounts.name AS customer_name , abms.aircraft_registrations.registration_number AS aircraft_registration_name, abms.aircraft_types.manufacturer, abms.billing_ledgers.amount_owing, abms.aircraft_registrations.aircraft_scope, abms.currencies.currency_code, abms.currencies.symbol
FROM abms.accounts
INNER JOIN abms.aircraft_registrations ON abms.accounts.id=abms.aircraft_registrations.account_id
INNER JOIN abms.aircraft_types ON abms.aircraft_types.id=abms.aircraft_registrations.aircraft_type_id
INNER JOIN abms.billing_ledgers ON abms.accounts.id=abms.billing_ledgers.account_id
INNER JOIN abms.currencies ON abms.currencies.id=abms.billing_ledgers.invoice_currency
INNER JOIN abms.transactions ON abms.currencies.id=abms.billing_ledgers.invoice_currency
WHERE abms.billing_ledgers.invoice_type = 'unified-tax' AND ( abms.transactions.transaction_date_time >= ? AND abms.transactions.transaction_date_time <= ?) AND  abms.accounts.id = any(?::int[]) 
 ]]></xml-property>
