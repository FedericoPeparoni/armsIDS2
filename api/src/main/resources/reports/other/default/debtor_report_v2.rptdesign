            <xml-property name="queryText"><![CDATA[SELECT 
id AS account_id,
name AS account_name,
amount_owing_current,
amount_owing_31_to_60,
amount_owing_61_to_90,
amount_owing_91_to_120,
amount_owing_121,
(amount_owing_current + amount_owing_31_to_60 + amount_owing_61_to_90 + amount_owing_91_to_120 + amount_owing_121) AS total
FROM (
SELECT account_id,
SUM(CASE WHEN DATE_PART('day', CURRENT_TIMESTAMP - payment_due_date) > 0 AND DATE_PART('day', CURRENT_TIMESTAMP - payment_due_date) < 31 THEN amount_owing * invoice_exchange_to_ansp ELSE 0.00 END) as amount_owing_current,
SUM(CASE WHEN DATE_PART('day', CURRENT_TIMESTAMP - payment_due_date) >= 31 AND DATE_PART('day', CURRENT_TIMESTAMP - payment_due_date) < 61 THEN amount_owing * invoice_exchange_to_ansp ELSE 0.00 END) as amount_owing_31_to_60,
SUM(CASE WHEN DATE_PART('day', CURRENT_TIMESTAMP - payment_due_date) >= 61 AND DATE_PART('day', CURRENT_TIMESTAMP - payment_due_date) < 91 THEN amount_owing * invoice_exchange_to_ansp ELSE 0.00 END) as amount_owing_61_to_90,
SUM(CASE WHEN DATE_PART('day', CURRENT_TIMESTAMP - payment_due_date) >= 91 AND DATE_PART('day', CURRENT_TIMESTAMP - payment_due_date) < 121 THEN amount_owing * invoice_exchange_to_ansp ELSE 0.00 END) as amount_owing_91_to_120,
SUM(CASE WHEN DATE_PART('day', CURRENT_TIMESTAMP - payment_due_date) >= 121 THEN amount_owing * invoice_exchange_to_ansp ELSE 0.00 END) AS amount_owing_121
FROM abms.billing_ledgers
GROUP BY account_id
) b 
LEFT OUTER JOIN abms.accounts a 
ON a.id = b.account_id
ORDER BY a.name
;]]></xml-property>
