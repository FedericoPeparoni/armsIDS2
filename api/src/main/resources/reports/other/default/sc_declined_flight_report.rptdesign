            <xml-property name="queryText"><![CDATA[SELECT account, 
       a.name AS account_name, 
       date_of_flight, 
       flight_id,
       aircraft_type, 
       item18_reg_num, 
       dep_ad, 
       dest_ad, 
       fpl_route, 
       billable_crossing_dist, 
       actual_mtow 
FROM   flight_movements 
       LEFT JOIN accounts a 
              ON a.id = flight_movements.account 
WHERE  status = 'DECLINED' 
       AND ( account IN (SELECT DISTINCT account_id 
                         FROM   account_users_map) 
              OR account IS NULL ) 
       AND date_of_flight BETWEEN ? AND ? 
ORDER BY item18_reg_num, date_of_flight, account]]></xml-property>
