{
  "name": "armsWeb",
  "version": "1.5.2",
  "dependencies": {
    "dotenv": "^4.0.0",
    "jspdf": "^1.3.3",
    "ts-xlsx": "0.0.8",
    "webpack-angular-translate": "^2.0.0"
  },
  "private": true,
  "scripts": {
    "test": "gulp test",
    "precommit": "gulp htmllint:test && validate-commit-msg",
    "prepush": "careful && gulp typescript:test && gulp test",
    "ci-install": "npm install && bower install --force-latest && typings install",
    "ci-test": "npm run ci-install && gulp config:inject && gulp typescript:test && gulp htmllint:test && gulp test",
    "ci-build": "npm run ci-install && npm rebuild node-sass && gulp config:inject && gulp build:config",
    "sonar-scanner": "sonar-scanner"
  },
  "config": {
    "careful": {
      "prefixes": [
        "feature",
        "fix",
        "hotfix",
        "release"
      ],
      "suggestions": {
        "features": "feature",
        "feat": "feature",
        "fixes": "fix",
        "bug": "fix",
        "bugs": "fix",
        "releases": "release"
      },
      "skip": [],
      "disallowed": [
        "master",
        "staging",
        "develop"
      ],
      "banned": [
        "wip"
      ],
      "seperator": "/"
    }
  },
  "devDependencies": {
    "browser-sync": "~2.9.11",
    "browser-sync-spa": "~1.0.3",
    "careful": "^1.1.0",
    "chalk": "~1.1.1",
    "del": "~2.0.2",
    "eslint-plugin-angular": "~0.12.0",
    "estraverse": "~4.1.0",
    "gulp": "~3.9.0",
    "gulp-angular-templatecache": "~1.8.0",
    "gulp-autoprefixer": "~3.0.2",
    "gulp-cssnano": "~2.1.1",
    "gulp-eslint": "~1.0.0",
    "gulp-filter": "~3.0.1",
    "gulp-flatten": "~0.2.0",
    "gulp-htmllint": "0.0.11",
    "gulp-htmlmin": "~1.3.0",
    "gulp-inject": "~3.0.0",
    "gulp-load-plugins": "~0.10.0",
    "gulp-protractor": "~2.1.0",
    "gulp-rename": "~1.2.2",
    "gulp-replace": "~0.5.4",
    "gulp-rev": "~6.0.1",
    "gulp-rev-replace": "~0.4.2",
    "gulp-sass": "~2.0.4",
    "gulp-size": "~2.0.0",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-typescript": "^3.1.3",
    "gulp-uglify": "~2.0.0",
    "gulp-useref": "~3.0.3",
    "gulp-util": "~3.0.6",
    "http-proxy-middleware": "~0.9.0",
    "husky": "^0.11.9",
    "jasmine-core": "~2.6.4",
    "karma": "~1.7.1",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.1.1",
    "karma-ng-html2js-preprocessor": "~1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "lodash": "~3.10.1",
    "main-bower-files": "~2.9.0",
    "ng-annotate-loader": "0.0.10",
    "protractor-http-mock": "^0.9.0",
    "sonar-scanner": "^3.0.3",
    "ts-loader": "~0.8.0",
    "tslint-loader": "~1.0.2",
    "typescript": "~2.3.2",
    "typings": "~1.4.x",
    "uglify-save-license": "~0.4.1",
    "validate-commit-message": "^3.0.1",
    "webpack-stream": "~2.1.1",
    "wiredep": "~2.2.2",
    "wrench": "~1.5.8",
    "yandex-translate": "^2.1.1"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
